        "editor.findMatchHighlightBorder": "#0f07",
        "editor.foldBackground": "#666",
        "editor.hoverHighlightBackground": "#fff4",
        "editor.inactiveSelectionBackground": "#4f43",
        "editor.lineHighlightBackground": "#fff2",
        "editor.lineHighlightBorder": "#0000",
        "editor.rangeHighlightBackground": "#fff2",
        "editor.rangeHighlightBorder": "#0000",
        "editor.selectionBackground": "#0f07",
        "editor.selectionHighlightBackground": "#0000",
        "editor.selectionHighlightBorder": "#0f07",
        "editor.wordHighlightBackground": "#0000",
        "editor.wordHighlightBorder": "#fa0",
        "editor.wordHighlightStrongBackground": "#0000",
        "editor.wordHighlightStrongBorder": "#f50",

        "editorIndentGuide.activeBackground": "#fffa",

        "editorBracketMatch.background": "#555",
        "editorBracketMatch.border": "#0000",

        "editorSuggestWidget.foreground": "#000",
