        "editorSuggestWidget.highlightForeground": "#eee",
        "editorSuggestWidget.selectedBackground": "#999",

        "editorOverviewRuler.findMatchForeground": "#0f0a",
        "editorOverviewRuler.bracketMatchForeground": "#fff",
        "editorOverviewRuler.wordHighlightForeground": "#fa0",
        "editorOverviewRuler.wordHighlightStrongForeground": "#f50",

        "editorCursor.foreground": "#000",

        "editorGutter.addedBackground": "#0f06",
        "editorGutter.deletedBackground": "#f006",
        "editorGutter.modifiedBackground": "#00f6",

        "editorGutter.background": "#666",
        "editorLineNumber.foreground": "#000",

        "editorGroupHeader.tabsBackground": "#666",

        "editorHoverWidget.background": "#888",

        "diffEditor.insertedTextBorder": "#0001",
        "diffEditor.removedTextBorder": "#0001",
        "diffEditor.insertedTextBackground": "#0f02",
        "diffEditor.removedTextBackground": "#f002",

        "dropdown.background": "#888",
        "dropdown.foreground": "#000",
        "dropdown.border": "#444",
        "dropdown.listBackground": "#777",

        "list.activeSelectionBackground": "#999",
