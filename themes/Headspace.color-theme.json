        "list.inactiveSelectionBackground": "#888",
        "list.focusBackground": "#999",

        "quickInput.background": "#777",
        "quickInput.foreground": "#000",

        "sideBar.background": "#777",
        "sideBar.dropBackground": "#0000",
        "sideBar.foreground": "#000",
        "sideBar.border": "#666",

        "sideBarSectionHeader.background": "#666",
        "sideBarSectionHeader.border": "#0000",
        "sideBarSectionHeader.foreground": "#000",

        "sideBarTitle.foreground": "#000",

        "activityBar.background": "#666",
        "activityBar.foreground": "#fff",
        "activityBar.activeBackground": "#0000",
        "activityBar.activeBorder": "#fff",
        "activityBar.inactiveForeground": "#000",

        "tab.activeBackground": "#888",
        "tab.activeForeground": "#fff",
        "tab.inactiveBackground": "#666",
        "tab.inactiveForeground": "#000",

        "gitDecoration.modifiedResourceForeground": "#000",
        "gitDecoration.untrackedResourceForeground": "#2d2",
        "gitDecoration.addedResourceForeground": "#000",
        "gitDecoration.conflictingResourceForeground": "#fa0",
        "gitDecoration.deletedResourceForeground": "#f66",
        "gitDecoration.ignoredResourceForeground": "#aaa",

        "gitlens.trailingLineForegroundColor": "#666",

        "breadcrumb.background": "#888",
        "breadcrumb.foreground": "#000",

        "panel.background": "#666",
        "panelTitle.activeForeground": "#fff",
        "panelTitle.inactiveForeground": "#000",

        "input.background": "#888",
        "input.foreground": "#000",
        "input.border": "#000",

        "minimap.selectionHighlight": "#0f0a",
        "minimap.findMatchHighlight": "#0f0a",

        "scrollbarSlider.background": "#fff3",

        "peekView.border": "#000",
        "peekViewEditor.background": "#777",
        "peekViewResult.background": "#666",
        "peekViewTitle.background": "#666",
        "peekViewTitleDescription.foreground": "#000",
        "peekViewTitleLabel.foreground": "#fff",

        "activityBarBadge.background": "#fff",
        "activityBarBadge.foreground": "#000",

        "button.background": "#999",
        "button.foreground": "#000",

        "selection.background": "#0f07",

        "badge.background": "#fff",
        "badge.foreground": "#000",

        "terminal.background": "#444",
        "terminal.foreground": "#ccc",

        "inputValidation.warningBackground": "#fa0",
        "inputValidation.warningBorder": "#fa0"
