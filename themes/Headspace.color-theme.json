    "tokenColors": [
        {
            "scope": [
                "entity",
                "variable",
                "meta.function-call.arguments"
            ],
            "settings": {
                "foreground": "#001"
            }
        },
        {
            "scope": [
                "punctuation",
            ],
            "settings": {
                "foreground": "#223",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "comment",
                "comment.block.documentation",
                "punctuation.definition.comment",
                "punctuation.definition.comment.tsx",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#445",
                "fontStyle": ""
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "foreground": "#0f0"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#f00"
            }
        },
        {
            "scope": [
                "meta",
                "variable.other",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#007"
            }
        },
        {
            "scope": [
                "keyword",
                "markup.inserted",
                "storage",
                "support",
                "meta.function-call",
                "punctuation.definition.template-expression",
                "constant.character.format"
            ],
            "settings": {
                "foreground": "#043"
            }
        },
        {
            "scope": [
                "keyword.control",
                "storage"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string",
            ],
            "settings": {
                "foreground": "#308"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "foreground": "#fa0"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#222"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "foreground": "#1aff79"
            }
        },
        {
            "scope": [
                "constant",
                "markup.deleted",
                "variable.other.object",
                // "variable.parameter"
            ],
            "settings": {
                "foreground": "#404"
            }
        }
    ]
}
